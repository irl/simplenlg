						<spec cat="DETERMINER">the</spec>
					</subj>
					<vp xsi:type="VPPhraseSpec" TENSE="PAST">
						<head cat="VERB">lie</head>
						<postMod xsi:type="VPPhraseSpec" TENSE="PAST">
							<head cat="VERB">etherise</head>
							<postMod xsi:type="PPPhraseSpec">
								<head cat="PREPOSITION">upon</head>
								<compl xsi:type="NPPhraseSpec">
									<head cat="NOUN">table</head>
									<spec cat="DETERMINER">a</spec>
								</compl>
							</postMod>
						</postMod>
					</vp>
				</child>
				<child xsi:type="SPhraseSpec">
					<subj xsi:type="NPPhraseSpec">
						<head cat="NOUN">patient</head>
						<spec cat="DETERMINER">the</spec>
					</subj>
					<vp xsi:type="VPPhraseSpec" TENSE="PAST">
						<head var="IRREGULAR" cat="VERB">lie</head>
						<postMod xsi:type="VPPhraseSpec" TENSE="PAST">
							<head cat="VERB">etherise</head>
							<postMod xsi:type="PPPhraseSpec">
								<head cat="PREPOSITION">upon</head>
								<compl xsi:type="NPPhraseSpec">
									<head cat="NOUN">table</head>
									<spec cat="DETERMINER">a</spec>
